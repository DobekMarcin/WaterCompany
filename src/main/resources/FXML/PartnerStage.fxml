<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="md.program.controller.PartnerStageController">
   <center>
      <TableView BorderPane.alignment="CENTER">
        <columns>
          <TableColumn minWidth="50.0" prefWidth="50.0" text="%partner.table.id" />
          <TableColumn minWidth="100.0" prefWidth="100.0" text="%partner.table.name" />
            <TableColumn minWidth="150.0" prefWidth="150.0" text="%partner.table.surname" />
            <TableColumn minWidth="200.0" prefWidth="200.0" text="%partner.table.address" />
            <TableColumn minWidth="50.0" prefWidth="50.0" text="%partner.table.people" />
            <TableColumn minWidth="50.0" prefWidth="75.0" text="%partner.table.archives" />
        </columns>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="%partner.table.add" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
            <Button layoutX="89.0" mnemonicParsing="false" text="%partner.table.delete" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="104.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin>
      </AnchorPane>
   </bottom>
</BorderPane>
